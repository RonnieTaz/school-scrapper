{
    "name": "megamindame/school_scraper",
    "description": "This package scrapes all Tanzania's schools from Necta's website.",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "files": [
            "helpers.php"
        ],
        "psr-4": {
            "Megamindame\\SchoolScraper\\": "src/"
        }
    },
    "authors": [
        {
            "name": "MegaMindAme",
            "email": "megamindame@gmail.com"
        }
    ],
    "require": {
        "php": ">= 7.4",
        "guzzlehttp/guzzle": "^7.7",
        "ext-libxml": "*",
        "ext-dom": "*",
        "symfony/dom-crawler": "^5.4"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require-dev": {
        "pestphp/pest": "^v1.23"
    }
}
